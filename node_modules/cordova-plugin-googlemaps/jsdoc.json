{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "www",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|__test__)"
    },
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
